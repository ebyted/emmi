{
    "buildType": "Dockerfile",
    "dockerfilePath": "Dockerfile",
    "context": ".",
    "services": {
      "web": {
        "labels": {
          "traefik.enable": "true",
          "traefik.http.routers.emmi.rule": "Host(`emmibrows.com`) || Host(`www.emmibrows.com`)",
          "traefik.http.routers.emmi.entrypoints": "websecure",
          "traefik.http.routers.emmi.tls": "true"
        }
      }
    }
  }